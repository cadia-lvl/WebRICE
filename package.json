{
  "name": "webrice",
  "version": "0.0.1",
  "description": "WebRICE is an open source web reader. It reads text aloud to users!",
  "main": "index.js",
  "scripts": {
    "browser-sync": "browser-sync start --server --files index.html styles.css",
    "sass": "node-sass main.scss styles.css -w",
    "compile": "tsc ./src/scripts.ts",
    "dev": "concurrently npm:sass npm:browser-sync",
    "slint": "stylelint \"main.scss\" --syntax scss",
    "elint": "eslint . --ext .js,.jsx,.ts,.tsx" 

  },
  "author": "Sunneva Þorsteinsdóttir and Judy Fong",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "browser-sync": "^2.26.7",
    "concurrently": "^5.2.0",
    "eslint": "^7.4.0",
    "eslint-config-google": "^0.14.0",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "node-sass": "^4.14.1",
    "stylelint-scss": "^3.18.0"
  }
}
